<Window x:Class="Batch_Rename.DuplicationDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Batch_Rename"
        mc:Ignorable="d"
        Title="Duplication" Width="276.5" Height="162.214" WindowStartupLocation="CenterScreen">
    <Canvas Background="LightYellow" >
        <TextBlock Text="Found duplication in the new names." Canvas.Left="10" Canvas.Top="5"/>
        <TextBlock  Text="Which option do you want ?" Canvas.Left="10" Canvas.Top="21"/>
        <Button Content="OK" Width="60" IsDefault="True" Canvas.Left="74" Canvas.Top="88" Name="Ok" Click="Ok_Click"/>
        <ComboBox Width="170" Canvas.Left="50" Canvas.Top="46" Name="DuplicationCombobox" SelectedIndex="0">
            <ComboBoxItem Content="Add number at the end"/>
            <ComboBoxItem Content="Skip"/>
        </ComboBox>
        <Button Content="Cancel" Width="60" IsCancel="True" Canvas.Left="160" Canvas.Top="88"/>
    </Canvas>
</Window>
